<div class="row">
    <img class="center-block img-circle" src=<%= @user.profile_picture %> />
    <p><h1 class="pull-left"><%= @user.username %></h1> <h3 class="pull-right"><%= link_to 'logout', :logout %></h3></p>
</div>
<h3><%= @user.bio %></h3>
<div class="col-sm-12 media">
    <% @media.each do |photo| %>
        <img src=<%= photo.images.low_resolution.url %>>
    <% end %>
</div>
